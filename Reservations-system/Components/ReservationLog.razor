@foreach (Reservation reservation in listOfReservation)
{<tr>
    <td>
        <button class="btn btn-primary" type="submit" @onclick="(() => ChosenReservation = reservation)">
            Bekræftet: @(reservation.Confirmed == true ? "Ja" : "Nej") <br />
            Tidspunkt: @reservation.ReservationPeriod() <br />
        </button>
    </td>
</tr>
}


@code {
    [CascadingParameter(Name = "chosenReservation")]
    public Reservation ChosenReservation { get; set; } 
    
    [CascadingParameter(Name = "dummyData")]
    public List<Reservation> listOfReservation { get; set; } //This is dummy data but should be changed when DB is set up
}
